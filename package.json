{
  "name": "express_app",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "test:workflow": "jest --forceExit --detectOpenHandles  --runInBand",
    "test:watch": "jest --detectOpenHandles --runInBand  --watchAll",
    "dev": "ts-node -- ./src/index.ts",
    "dev:watch": "nodemon --exec ts-node -- ./src/index.ts | bunyan",
    "docker:development": "docker-compose -f docker-compose.development.yml up",
    "docker:production": "docker-compose -f docker-compose.production.yml up  -d --build",
    "production:build": " tsc -p tsconfig.json",
    "production:pm2": "NODE_ENV=production pm2 start build/index.js --watch -i max",
    "production:start": "NODE_ENV=production node build/index.js ",
    "utils:clean": "sh ./scripts/clean.sh",
    "utils:clean-hard": "sh ./scripts/clean.sh --hard",
    "utils:clean-jest": "jest --clearCache",
    "utils:format": "sh ./scripts/format.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "jest": "^28.0.1",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}